{
  "name": "lito",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "ahooks": "^2.10.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.1"
  },
  "scripts": {
    "start": "concurrently -k \"npm:start:react\" \"npm:start:windows\"",
    "start:react": "cross-env BROWSER=none react-scripts start",
    "start:windows": "wait-on tcp:3000 && cd windows && cargo run",
    "build": "npm run build:react && npm run build:windows",
    "build:react": "react-scripts build",
    "build:windows": "cd windows && cargo build --target x86_64-pc-windows-msvc --release",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/apple-music-api": "^0.4.1",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.14",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "prettier": "^2.4.0",
    "prettier-plugin-organize-imports": "^2.3.3",
    "prettier-plugin-style-order": "^0.2.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.2",
    "wait-on": "^6.0.0"
  }
}
